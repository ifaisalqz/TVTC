declare
    NUMBER_ONE integer(20);
    NUMBER_TWO integer(20);
    RESULT_NUM integer(20);
begin
    NUMBER_ONE:=:NUMBER_ONE;
    NUMBER_TWO:=:NUMBER_TWO;
    
    RESULT_NUM := (NUMBER_ONE + NUMBER_TWO);

    dbms_output.put_line('Number1 , ' || NUMBER_ONE);
    dbms_output.put_line('Number2 , ' || NUMBER_TWO);
    dbms_output.put_line('Result , ' || RESULT_NUM);  
END;
